{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ServerOptions": {
    "Name": "PerformanceTest.Server",
    "ClearIdleSessionInterval": 30, //30秒检测一次空闲客户端
    "IdleSessionTimeOut": 20, //清理超过20秒无活动的客户端
    "Listeners": [
      {
        "Ip": "Any",
        "Port": 4040
      }
    ]
  }
}
