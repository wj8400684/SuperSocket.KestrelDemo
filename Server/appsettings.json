{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ServerOptions": {
    "Name": "PerformanceTest.Server",
    //最大接受封包 1m
    "MaxPackageLength": 1048576,
    "ReceiveBufferSize": 8192,
    "SendBufferSize": 8192,
    "ReceiveTimeout": 1000,
    "SendTimeout": 1000,
    "ClearIdleSessionInterval": 30000, //30秒无连接则清理
    "Listeners": [
      {
        "Ip": "Any",
        "Port": 4040,
        "BackLog": 65535
      }
    ]
  }
}
